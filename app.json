{
  "expo": {
    "name": "OVOClone",
    "slug": "OVOClone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/ovo.png",
      "resizeMode": "contain",
      "backgroundColor": "#4c2a86"
    },
    "assetBundlePatterns": ["**/*"],
    "userInterfaceStyle": "automatic",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
